    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "svelte": "^5.38.0",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "graphql": "^16.11.0",
